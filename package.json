{
  "name": "@mtti/nominal-uuid",
  "version": "1.0.1",
  "description": "Nominal UUID types for Typescript",
  "repository": "github:mtti/nominal-uuid",
  "author": "Matti Hiltunen",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=12.13.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "jest": "jest --config ./jest.config.json",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "setup": "npm install",
    "test": "npm run lint && npm run jest",
    "test:docker": "docker build -f ./Dockerfile-test -t ts-test . && docker run --rm -it ts-test",
    "tsc": "tsc",
    "watch": "chokidar --initial -c \"npm run build\" \"src/**/*.ts\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "files": [
    "lib",
    "!lib/**/*.test.*"
  ],
  "devDependencies": {
    "@mtti/eslint-config-typescript": "3.0.0",
    "@types/jest": "26.0.20",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "chokidar-cli": "2.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "ts-jest": "26.5.1",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
